<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"

    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.korellia.korelliaclubapp.ui.home.HomeFragment"
        android:label="Korellia Club"

        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.korellia.korelliaclubapp.ui.dashboard.DashboardFragment"
        android:label="Entrenamiento"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.korellia.korelliaclubapp.ui.notifications.NotificationsFragment"
        android:label="Notificaciones"
        tools:layout="@layout/fragment_notifications" />

    <fragment
        android:id="@+id/progresos"
        android:name="com.korellia.korelliaclubapp.progresos"
        android:label="Progresos"
        tools:layout="@layout/fragment_progresos" />

    <fragment
